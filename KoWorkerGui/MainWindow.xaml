<Window x:Class="KoWorkerGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoWorkerGui"
        mc:Ignorable="d"
        Title="KoWorkers" Height="350" Width="525">
    <Grid>
        <Label x:Name="Welcome_Label" Content="Velkommen til KoWorkers" HorizontalAlignment="Left" Height="24" Margin="169,10,0,0" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="Admin_Button" Content="Medarbejderadministration" HorizontalAlignment="Left" Height="38" Margin="145,75,0,0" VerticalAlignment="Top" Width="183" BorderBrush="#FFD6CFCF" Background="White"/>
        <Button x:Name="Admin_Button_Copy" Content="Medarbejderadministration" HorizontalAlignment="Left" Height="38" Margin="145,75,0,0" VerticalAlignment="Top" Width="183" BorderBrush="#FFD6CFCF" Background="White" Click="Admin_Button_Copy_Click"/>
        <ListBox HorizontalAlignment="Left" Height="190" VerticalAlignment="Top" Width="520" Margin="0,130,-3,-1">
            <Label x:Name="Label_TjekIndUd" Content="Tjek ind/ud" Width="105"/>
        </ListBox>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="105" Margin="5,135,0,0"/>

    </Grid>
</Window>
